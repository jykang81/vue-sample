{  
  "workbench.iconTheme": "material-icon-theme",
  "editor.tabSize": 2,
  "vetur.validation.template": false,
  "eslint.validate": [
      "vue",
      "javascript",
      "javascriptreact"    
  ],
  "eslint.workingDirectories": ["."],
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "[css]": {
    "editor.tabSize": 4
  },
  "[scss]": {
    "editor.tabSize": 4
  },
  "[html]": {
      "editor.tabSize": 2
  },
  "[javascript]": {
      "editor.tabSize": 2
  },
  "editor.codeActionsOnSave": {
    "source.fixAll": true
  },
  "files.eol": "\n"
}